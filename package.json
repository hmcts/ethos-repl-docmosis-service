{
  "name": "EthosProject",
  "description": "End to End test for Probate Back Office",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "lint": "NODE_PATH=. eslint .",
    "eslint": "NODE_PATH=. eslint .",
    "test-e2e": "NODE_PATH=. node ./node_modules/codeceptjs/bin/codecept.js run -c ./src/test/end-to-end/ --steps",
    "test-crossbrowser-e2e": "NODE_PATH=. codeceptjs run -c src/test/end-to-end/saucelabs.conf.js --steps",
    "test:fullfunctional": "NODE_PATH=. node ./node_modules/codeceptjs/bin/codecept.js run-multiple parallel -c ./src/test/end-to-end/ --steps --reporter mochawesome",
    "test-ui-cypress": "NODE_PATH=. node ./node_modules/.bin/cypress run --headless"
  },
  "dependencies": {
    "@hapi/hoek": "^9.2.0",
    "cypress": "^7.7.0",
    "randomstring": "^1.2.1",
    "require-directory": "^2.1.1"
  },
  "devDependencies": {
    "codeceptjs": "^3.0.7",
    "eslint": "7.30.0",
    "eslint-plugin-mocha": "9.0.0",
    "istanbul": "0.4.5",
    "mocha": "^9.0.2",
    "mocha-jenkins-reporter": "^0.4.5",
    "mocha-multi": "^1.1.3",
    "mochawesome": "6.2.2",
    "nock": "13.1.1",
    "nodemon": "^2.0.12",
    "pa11y": "6.0.1",
    "phantomjs-prebuilt": "^2.1.16",
    "proxyquire": "^2.1.3",
    "puppeteer": "10.1.0",
    "rewire": "5.0.0",
    "selenium-standalone": "7.1.0",
    "sinon": "11.1.1",
    "sinon-chai": "^3.7.0",
    "sonar-scanner": "^3.1.0",
    "sonarqube-scanner": "^2.8.1",
    "superagent-proxy": "^2.1.0",
    "supertest": "6.1.3",
    "unirest": "^0.6.0",
    "uuid": "8.3.2",
    "watch": "^0.13.0",
    "webdriverio": "^7.7.6",
    "when": "^3.7.8"
  }
}
